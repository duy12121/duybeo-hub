MONGO_URI=mongodb+srv://user:password@cluster.mongodb.net/zalo_bot_manager?retryWrites=true&w=majority
DATABASE_NAME=zalo_bot_manager

SECRET_KEY=your-super-secret-key-change-this-in-production-min-32-chars
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=1440

# CORS Origins - comma separated list of allowed domains
CORS_ORIGINS=http://localhost:3000,http://localhost:5173,https://your-production-domain.com

BOT_API_KEY=your-bot-api-key-here

ZALO_API_KEY=your-zalo-api-key
ZALO_SECRET_KEY=your-zalo-secret-key
ZALO_IMEI=your-device-imei
ZALO_COOKIES={}
AUTO_START_BOT=false

# Gemini AI - Multiple API keys for rotation (comma-separated)
# Format: GEMINI_KEYS=key1,key2,key3
GEMINI_KEYS=your-gemini-api-key-1,your-gemini-api-key-2,your-gemini-api-key-3

# Default admin credentials (CHANGE IN PRODUCTION)
DEFAULT_ADMIN_USERNAME=admin
DEFAULT_ADMIN_PASSWORD=change-this-secure-password
DEFAULT_ADMIN_EMAIL=admin@yourdomain.com

# Backend URL for bot to report activity/logs (optional; default http://localhost:8000)
# BACKEND_URL=http://localhost:8000
